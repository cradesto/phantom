{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.un~": true,
    "**/*.o": true,
    "**/*.mod": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*.sh": true
  },
  "fortran.linter.includePaths": [
    "${workspaceFolder}/**/**",
  ],
  "fortran.linter.modOutput": "${workspaceFolder}/.fmod",
  "explorerExclude.backup": {},
  "python.defaultInterpreterPath": "${env:PYTHON_INSTALL_LOC}/plonk/bin/python",
  "editor.detectIndentation": false,
  "todo-tree.tree.scanMode": "workspace"
}
